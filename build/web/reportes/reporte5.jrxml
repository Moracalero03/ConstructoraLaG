	<queryString>
		<![CDATA[select p.idProyecto, p.proyecto, p.FechaInicio, p.EstadoP, p.comentario, p.costoInicial, p.costoFinal, p.FechaFinal from proyecto p
inner join municipio m
on p.idMunicipio=m.idMunicipio
inner join departamento d
on m.idDepartamento=d.idDepartamento
where d.departamento=$P{depa} and p.EstadoP=$P{estado};]]>
	</queryString>
